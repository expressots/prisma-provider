// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = ""
}

model User {
  id String @id
  email String @unique
  updatedAt DateTime @updatedAt
  profile Profile?

  @@index([email])
}

model Profile {
  id Int @id
  bio String @unique
}
