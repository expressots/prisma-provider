// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = ""
}

model User {
  id String @id
  name String
  email String
  age String?
  posts Post[]

  @@unique([name, email])
}

model Post {
  id String @id
  title String
  content String
user User @relation(fields: [postId], references: [authorId])

}
