generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = ""
}

// [Types]

// [Enums]

// [Models]

model User {
  id String @id @db.Text @default(uuid())
  name String @db.Text
  role Role
}




