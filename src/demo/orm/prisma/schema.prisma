// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = ""
}

// [Types]

// [Enums]

enum Role {
  ADMIN
}

enum Color {
  RED
  GREEN
  BLUE
}

// [Models]

model User {
  id String @id @db.Text @default(uuid())
  name String
  color Color[]
  role Role
  photos BigInt
}
