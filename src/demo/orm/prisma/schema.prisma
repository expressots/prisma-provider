// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = ""
}

enum Color {
  RED
  GREEN
  BLUE
}

model Post {
  id   String @id @default(uuid()) @map("_id")
  name String
  role Role
}

enum Role {
  ADMIN
}

model User {
  id    String  @id @default(uuid()) @map("_id")
  name  String
  color Color[]
  role  Role
  money Decimal @db.Decimal(10, 3)
}
