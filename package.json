{
    "name": "@expressots/prisma",
    "version": "0.0.1-rc1",
    "description": "Prisma provider for ExpressoTS",
    "author": "Richard Zampieri",
    "keywords": [
        "expressots",
        "nodejs",
        "orm",
        "typescript",
        "prisma",
        "provider",
        "sql",
        "nosql"
    ],
    "main": "./lib/cjs/index.js",
    "types": "./lib/cjs/types/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./lib/esm/types/index.d.ts",
                "default": "./lib/esm/index.mjs"
            },
            "require": {
                "types": "./lib/cjs/types/index.d.ts",
                "default": "./lib/cjs/index.js"
            }
        }
    },
    "files": [
        "lib/**/*",
        "bin/**/*"
    ],
    "license": "MIT",
    "homepage": "https://expresso-ts.com",
    "funding": {
        "type": "sponsors",
        "url": "https://github.com/sponsors/expressots"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/expressots/prisma-provider"
    },
    "bugs": {
        "url": "https://github.com/expressots/prisma-provider/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=18.10.0"
    },
    "bin": {
        "expressots-prisma": "bin/cli/cli.js"
    },
    "scripts": {
        "clean": "rm -rf lib && rm -rf bin",
        "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:cli",
        "build:esm": "tsc -p tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "build:cli": "tsc -p tsconfig.cli.json && chmod +x ./bin/cli/cli.js",
        "start:dev": "tsnd --respawn src/cli/cli.ts",
        "dry:run": "npm publish --dry-run",
        "publish": "npm publish --access public",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint \"src/**/*.ts\" --fix",
        "test": "jest --verbose --detectOpenHandles",
        "test:coverage": "jest --coverage --detectOpenHandles",
        "prepare": "husky install",
        "lint-staged": "lint-staged"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.7.2",
        "@commitlint/config-conventional": "^17.7.0",
        "@prisma/client": "4.16.1",
        "@types/glob": "8.1.0",
        "@types/jest": "^29.5.5",
        "@typescript-eslint/eslint-plugin": "5.62.0",
        "@typescript-eslint/parser": "5.62.0",
        "eslint": "8.50.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-prettier": "4.2.1",
        "husky": "^8.0.3",
        "jest": "29.7.0",
        "lint-staged": "^14.0.1",
        "prettier": "3.0.3",
        "prisma": "4.16.1",
        "ts-jest": "^29.1.1",
        "ts-node-dev": "2.0.0",
        "typescript": "5.2.2"
    },
    "dependencies": {
        "chalk": "4.1.2",
        "class-transformer": "0.5.1",
        "reflect-metadata": "0.1.13",
        "yargs": "17.7.2"
    },
    "prisma": {
        "schema": "./src/test/schema.prisma"
    }
}
