{
  "name": "@expressots/prisma",
  "version": "1.0.0",
  "description": "Prisma provider for Expresso TS",
  "author": "Richard Zampieri",
  "keywords": [
    "expressots",
    "nodejs",
    "orm",
    "typescript",
    "prisma",
    "provider",
    "sql",
    "nosql"
  ],
  "main": "./lib/cjs/index.js",
  "types": "./lib/cjs/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "files": [
    "lib/**/*"
  ],
  "license": "MIT",
  "homepage": "https://expresso-ts.com",
  "funding": {
    "type": "",
    "url": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/expressots/prisma-provider"
  },
  "bugs": {
    "url": "https://github.com/expressots/prisma-provider/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.10.0"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "start:dev": "tsnd --respawn src/cli/cli.ts",
    "dry:run": "npm publish --dry-run",
    "publish": "npm publish --access public",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.2",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.6.0",
    "prettier": "2.8.8",
    "prisma": "4.16.1",
    "ts-jest": "29.1.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@prisma/client": "4.16.1",
    "chalk": "4.1.2",
    "class-transformer": "0.5.1",
    "reflect-metadata": "0.1.13",
    "yargs": "^17.7.2"
  },
  "prisma": {
    "schema": "./src/test/schema.prisma"
  }
}
